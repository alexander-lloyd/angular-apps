{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "todo": {
      "projectType": "application",
      "root": "apps/todo",
      "sourceRoot": "apps/todo/src",
      "prefix": "al",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/todo",
            "index": "apps/todo/src/index.html",
            "main": "apps/todo/src/main.ts",
            "polyfills": "apps/todo/src/polyfills.ts",
            "tsConfig": "apps/todo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/todo/src/assets",
              "apps/todo/src/manifest.webmanifest",
              "apps/todo/src/manifest.json"
            ],
            "styles": ["apps/todo/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/todo/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todo/src/environments/environment.ts",
                  "with": "apps/todo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "subresourceIntegrity": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "index": {
                "input": "apps/todo/src/index.production.html",
                "output": "index.html"
              },
              "serviceWorker": true,
              "ngswConfigPath": "apps/todo/ngsw-config.json"
            },
            "chrome-extension": {
              "fileReplacements": [
                {
                  "replace": "apps/todo/src/environments/environment.ts",
                  "with": "apps/todo/src/environments/environment.extension.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "subresourceIntegrity": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "index": {
                "input": "apps/todo/src/index.production.html",
                "output": "index.html"
              },
              "serviceWorker": false,
              "styles": [
                "apps/todo/src/styles.scss",
                "apps/todo/src/extension.css"
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "todo:build",
            "extraWebpackConfig": "./apps/todo-e2e/webpack.config.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "todo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/todo/src/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todo/jest.config.js"
          },
          "outputs": ["coverage/apps/todo"]
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/todo/",
            "configuration": "production",
            "repo": "https://github.com/alexander-lloyd/todo",
            "branch": "master",
            "name": "Alex Bot",
            "email": "bot@alexander-lloyd.dev",
            "cname": ""
          }
        }
      }
    },
    "todo-e2e": {
      "root": "apps/todo-e2e",
      "sourceRoot": "apps/todo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todo-e2e/cypress.json",
            "tsConfig": "apps/todo-e2e/tsconfig.e2e.json",
            "devServerTarget": "todo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todo-e2e/src/**/*.ts",
              "apps/todo-e2e/src/**/*.js",
              "apps/todo-e2e/**/*.ts",
              "apps/todo-e2e/**/*.js"
            ]
          }
        }
      }
    },
    "favourite": {
      "projectType": "application",
      "root": "apps/favourite",
      "sourceRoot": "apps/favourite/src",
      "prefix": "al",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/favourite",
            "index": "apps/favourite/src/index.html",
            "main": "apps/favourite/src/main.ts",
            "polyfills": "apps/favourite/src/polyfills.ts",
            "tsConfig": "apps/favourite/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/favourite/src/favicon.ico",
              "apps/favourite/src/assets"
