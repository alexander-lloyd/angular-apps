{
  "version": 1,
  "projects": {
    "todo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/todo",
      "sourceRoot": "apps/todo/src",
      "prefix": "al",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todo",
            "index": "apps/todo/src/index.html",
            "main": "apps/todo/src/main.ts",
            "polyfills": "apps/todo/src/polyfills.ts",
            "tsConfig": "apps/todo/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/todo/src/favicon.ico", "apps/todo/src/assets"],
            "styles": ["apps/todo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todo/src/environments/environment.ts",
                  "with": "apps/todo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "todo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/todo/.eslintrc.js",
            "tsConfig": [
              "apps/todo/tsconfig.app.json",
              "apps/todo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/todo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todo/jest.config.js",
            "tsConfig": "apps/todo/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/todo/src/test-setup.ts"
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/todo/",
            "configuration": "production",
            "repo": "https://github.com/alexander-lloyd/todo",
            "branch": "master",
            "name": "Alex Bot",
            "email": "bot@alexander-lloyd.dev"
          }
        }
      }
    },
    "todo-e2e": {
      "root": "apps/todo-e2e",
      "sourceRoot": "apps/todo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todo-e2e/cypress.json",
            "tsConfig": "apps/todo-e2e/tsconfig.e2e.json",
            "devServerTarget": "todo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/todo-e2e/.eslintrc.js",
            "tsConfig": ["apps/todo-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/todo-e2e/**"]
          }
        }
      }
    },
    "logger": {
      "projectType": "library",
      "root": "libs/logger",
      "sourceRoot": "libs/logger/src",
      "prefix": "al",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/logger/tsconfig.lib.json",
            "project": "libs/logger/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/logger/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/todo-e2e/.eslintrc.js",
            "tsConfig": [
              "libs/logger/tsconfig.lib.json",
              "libs/logger/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/logger/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/logger/jest.config.js",
            "tsConfig": "libs/logger/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/logger/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "spotify-player": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/spotify-player",
      "sourceRoot": "apps/spotify-player/src",
      "prefix": "al",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/spotify-player",
            "index": "apps/spotify-player/src/index.html",
            "main": "apps/spotify-player/src/main.ts",
            "polyfills": "apps/spotify-player/src/polyfills.ts",
            "tsConfig": "apps/spotify-player/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/spotify-player/src/favicon.ico",
              "apps/spotify-player/src/assets"
            ],
            "styles": ["apps/spotify-player/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/spotify-player/src/environments/environment.ts",
                  "with": "apps/spotify-player/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "index": {
                "input": "apps/spotify-player/src/index.production.html",
                "output": "index.html"
              }
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "spotify-player:build",
            "extraWebpackConfig": "./apps/spotify-player-e2e/coverage.webpack.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "spotify-player:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "spotify-player:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/spotify-player/.eslintrc.js",
            "tsConfig": [
              "apps/spotify-player/tsconfig.app.json",
              "apps/spotify-player/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/spotify-player/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/spotify-player/jest.config.js",
            "tsConfig": "apps/spotify-player/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/spotify-player/src/test-setup.ts"
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/spotify-player/",
            "configuration": "production",
            "repo": "https://github.com/alexander-lloyd/spotify-player",
            "branch": "master",
            "name": "Alex Bot",
            "email": "bot@alexander-lloyd.dev"
          }
        }
      }
    },
    "spotify-player-e2e": {
      "root": "apps/spotify-player-e2e",
      "sourceRoot": "apps/spotify-player-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spotify-player-e2e/cypress.json",
            "tsConfig": "apps/spotify-player-e2e/tsconfig.e2e.json",
            "devServerTarget": "spotify-player:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "spotify-player:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/spotify-player-e2e/.eslintrc.js",
            "tsConfig": ["apps/spotify-player-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/spotify-player-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "todo"
}
